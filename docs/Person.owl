<?xml version="1.0"?>
<rdf:RDF xmlns="https://w3id.org/isoiec5087-2/v1/"
     xml:base="https://w3id.org/isoiec5087-2/v1/"
     xmlns:cc="http://creativecommons.org/ns#"
     xmlns:dcterms="http://purl.org/dc/terms/"
     xmlns:owl="http://www.w3.org/2002/07/owl#"
     xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
     xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
     xmlns:skos="http://www.w3.org/2004/02/skos/core#"
     xmlns:vann="http://purl.org/vocab/vann/"
     xmlns:xml="http://www.w3.org/XML/1998/namespace"
     xmlns:xsd="http://www.w3.org/2001/XMLSchema#"
     xmlns:i72="https://w3id.org/isoiec21972/v1/"
     xmlns:cdm1="https://w3id.org/isoiec5087-1/v1/">
    <owl:Ontology rdf:about="https://w3id.org/isoiec5087-2/v1/PersonPattern">
        <!-- Identity -->
        <dcterms:title>Information technology - City data model - Part 2: City level concepts - Person pattern</dcterms:title>
        <rdfs:seeAlso rdf:resource="https://www.iso.org/standard/80742.html"/>
        <dcterms:bibliographicCitation>ISO/IEC 5087-2:2024</dcterms:bibliographicCitation>
    	<skos:definition>This ontology specifies the city-level concepts for the person pattern of the city data model.</skos:definition>
    	<vann:preferredNamespaceUri>https://w3id.org/isoiec5087-2/v1/</vann:preferredNamespaceUri>
    	<vann:preferredNamespacePrefix>cdm2</vann:preferredNamespacePrefix>

    	<!-- Provenance -->
        <dcterms:creator>Megan Katsumi</dcterms:creator>
        <dcterms:creator>Mark Fox</dcterms:creator>
    	<dcterms:creator>Kenneth Vaughn</dcterms:creator>
        <dcterms:issued rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2024</dcterms:issued> <!-- Date of the source document, comment out if draft -->
        <owl:priorVersion rdf:resource="https://standards.iso.org/iso-iec/5087/-2/ed-1/en/ontology/Person/"/>
        <owl:priorVersion rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2021-01-11</owl:priorVersion> <!-- Date of prior ontology file, if any -->
		<dcterms:modified rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2026-02-08</dcterms:modified> <!-- Date of this ontology file -->
        <owl:versionInfo>1.3.0</owl:versionInfo>
        <owl:versionIRI rdf:resource="https://w3id.org/isoiec5087-2/v1/r3.0"/>
        <rdfs:comment>The prior version of this ontology used a different ontology IRI. This update remains semantically consistent with the version adopted as ISO 5087-2:2024.</rdfs:comment>
        
        <!-- License -->
        <cc:license>http://creativecommons.org/licenses/by/4.0/</cc:license>
        
        <!-- Imports -->
        <owl:imports rdf:resource="https://w3id.org/isoiec5087-2/v1/ContactPattern"/>
    </owl:Ontology>
    

    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Object Properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    
    <!-- https://w3id.org/isoiec5087-2/v1/PersonObjectProperty -->

    <owl:ObjectProperty rdf:about="https://w3id.org/isoiec5087-2/v1/PersonObjectProperty">
        <skos:definition>Added for organizational purposes, to identify object properties defined for the Person Pattern in the City-level CDM ontology.</skos:definition>
        <rdfs:subPropertyOf rdf:resource="https://w3id.org/isoiec5087-2/v1/CityObjectProperty"/>
    </owl:ObjectProperty>
    

    <!-- Issue: Originally, this was limited to PersonName, but other entities can have aliases as well (e.g., roads). I have defined the range to be a Name, which can then be specialized to a PersonName or RoadName. -->
    <!-- https://w3id.org/isoiec5087-2/v1/alias -->

    <owl:ObjectProperty rdf:about="https://w3id.org/isoiec5087-2/v1/alias">
        <skos:definition>Identifies any alternative names associated with a Person. </skos:definition>
        <skos:note>A Person can only have one legal name, but can have multiple alias names.</skos:note>
        <rdfs:subPropertyOf rdf:resource="https://w3id.org/isoiec5087-2/v1/PersonObjectProperty"/>
        <rdfs:range rdf:resource="https://w3id.org/isoiec5087-2/v1/Name"/>
    </owl:ObjectProperty>
    

    <!-- Issue: standard does not include the invariant restriction -->
    <!-- https://w3id.org/isoiec5087-2/v1/birthDate -->

    <owl:ObjectProperty rdf:about="https://w3id.org/isoiec5087-2/v1/birthDate">
        <skos:definition>Identifies the time:Instant when the Person was born.</skos:definition>
        <rdfs:subPropertyOf rdf:resource="https://w3id.org/isoiec5087-2/v1/PersonObjectProperty"/>
        <cdm1:isInvariant rdf:datatype="http://www.w3.org/2001/XMLSchema#boolean">true</cdm1:isInvariant>
        <rdfs:range rdf:resource="http://www.w3.org/2006/time#Instant"/>
    </owl:ObjectProperty>
    

    <!-- https://w3id.org/isoiec5087-2/v1/birthplace -->

    <owl:ObjectProperty rdf:about="https://w3id.org/isoiec5087-2/v1/birthplace">
        <skos:definition>Identifies the place where the Person was born.</skos:definition>
        <rdfs:subPropertyOf rdf:resource="https://w3id.org/isoiec5087-2/v1/PersonObjectProperty"/>
        <rdfs:range rdf:resource="https://w3id.org/isoiec5087-2/v1/Address"/>
    </owl:ObjectProperty>
    

    <!-- https://w3id.org/isoiec5087-2/v1/children -->

    <owl:ObjectProperty rdf:about="https://w3id.org/isoiec5087-2/v1/children">
        <skos:definition>Identifies any children of a person.</skos:definition>
        <rdfs:subPropertyOf rdf:resource="https://w3id.org/isoiec5087-2/v1/PersonObjectProperty"/>
        <rdfs:range rdf:resource="https://w3id.org/isoiec5087-2/v1/Person"/>
    </owl:ObjectProperty>
    

    <!-- https://w3id.org/isoiec5087-2/v1/citizenOf -->

    <owl:ObjectProperty rdf:about="https://w3id.org/isoiec5087-2/v1/citizenOf">
        <skos:definition>specifies one or more Citizenships, each specifying the country (Country) and time interval (time:ProperInterval) the Person is a citizen. A Person can be a citizen of more than one country and for different time intervals. It is recommended that a Country be defined with using ISO 3166-2 [4] alpha-2 (2-letter) country code, however different systems may be accommodated with the Code pattern.</skos:definition>
        <rdfs:subPropertyOf rdf:resource="https://w3id.org/isoiec5087-2/v1/PersonObjectProperty"/>
        <rdfs:range rdf:resource="https://w3id.org/isoiec5087-2/v1/Citizenship"/>
    </owl:ObjectProperty>
    

    <!-- https://w3id.org/isoiec5087-2/v1/deathDate -->

    <owl:ObjectProperty rdf:about="https://w3id.org/isoiec5087-2/v1/deathDate">
        <skos:definition>Identifies the time:Instant when the Person died.</skos:definition>
        <rdfs:subPropertyOf rdf:resource="https://w3id.org/isoiec5087-2/v1/PersonObjectProperty"/>
        <rdfs:range rdf:resource="http://www.w3.org/2006/time#Instant"/>
    </owl:ObjectProperty>
    

    <!-- https://w3id.org/isoiec5087-2/v1/deathPlace -->

    <owl:ObjectProperty rdf:about="https://w3id.org/isoiec5087-2/v1/deathPlace">
        <skos:definition>Identifies the place where the Person died.</skos:definition>
        <rdfs:subPropertyOf rdf:resource="https://w3id.org/isoiec5087-2/v1/PersonObjectProperty"/>
        <rdfs:range rdf:resource="https://w3id.org/isoiec5087-2/v1/Address"/>
    </owl:ObjectProperty>
    

    <!-- https://w3id.org/isoiec5087-2/v1/forCountry -->

    <owl:ObjectProperty rdf:about="https://w3id.org/isoiec5087-2/v1/forCountry">
        <skos:definition>Identifies the country for which the Person is a citizen for the given dates.</skos:definition>
        <rdfs:subPropertyOf rdf:resource="https://w3id.org/isoiec5087-2/v1/PersonObjectProperty"/>
        <rdfs:range rdf:resource="https://w3id.org/isoiec5087-2/v1/Country"/>
    </owl:ObjectProperty>
    

    <!-- Issue: ISO/IEC 5087-2 does not define the forPerson property and it is unclear what the purpose of this is. Recommend deletion -->
    <!-- https://w3id.org/isoiec5087-2/v1/forPerson -->

<!--    <owl:ObjectProperty rdf:about="https://w3id.org/isoiec5087-2/v1/forPerson">
        <rdfs:subPropertyOf rdf:resource="https://w3id.org/isoiec5087-2/v1/PersonObjectProperty"/>
    </owl:ObjectProperty>  -->
    

    <!-- Issue: ISO/IEC 5087-2 does not define the hasAge property and it is unclear what the purpose of this is given that there is a birthdate. Recommend deletion -->
    <!-- https://w3id.org/isoiec5087-2/v1/hasAge -->

<!--    <owl:ObjectProperty rdf:about="https://w3id.org/isoiec5087-2/v1/hasAge">
        <rdfs:subPropertyOf rdf:resource="https://w3id.org/isoiec5087-2/v1/PersonObjectProperty"/>
    </owl:ObjectProperty>  -->
    

    <!-- https://w3id.org/isoiec5087-2/v1/hasEducation -->

    <owl:ObjectProperty rdf:about="https://w3id.org/isoiec5087-2/v1/hasEducation">
        <skos:definition>Identifies the education of a person.</skos:definition>
        <rdfs:subPropertyOf rdf:resource="https://w3id.org/isoiec5087-2/v1/PersonObjectProperty"/>
        <rdfs:range rdf:resource="https://w3id.org/isoiec5087-2/v1/Education"/>
    </owl:ObjectProperty>
    

    <!-- https://w3id.org/isoiec5087-2/v1/hasGenderIdentity -->

    <owl:ObjectProperty rdf:about="https://w3id.org/isoiec5087-2/v1/hasGenderIdentity">
        <skos:definition>Identifies the gender identity of a person.</skos:definition>
        <rdfs:subPropertyOf rdf:resource="https://w3id.org/isoiec5087-2/v1/PersonObjectProperty"/>
        <rdfs:range rdf:resource="https://w3id.org/isoiec5087-2/v1/Gender"/>
    </owl:ObjectProperty>
    

    <!-- https://w3id.org/isoiec5087-2/v1/hasIDType -->

    <owl:ObjectProperty rdf:about="https://w3id.org/isoiec5087-2/v1/hasIDType">
        <skos:definition>Specifies the type of identifier, its values may be defined more precisely with the use of the code:hasCode property.</skos:definition>
        <rdfs:subPropertyOf rdf:resource="https://w3id.org/isoiec5087-2/v1/PersonObjectProperty"/>
        <rdfs:range rdf:resource="https://w3id.org/isoiec5087-2/v1/IDType"/>
    </owl:ObjectProperty>
    

    <!-- Issue: standard does not include the invariant restriction -->
    <!-- https://w3id.org/isoiec5087-2/v1/hasPersonID -->

    <owl:ObjectProperty rdf:about="https://w3id.org/isoiec5087-2/v1/hasPersonID">
        <skos:definition>Identifies any instances of a Person’s PersonId.</skos:definition>
        <skos:example>driver's license</skos:example>
        <skos:example>passport</skos:example>
        <rdfs:subPropertyOf rdf:resource="https://w3id.org/isoiec5087-2/v1/PersonObjectProperty"/>
        <cdm1:isInvariant rdf:datatype="http://www.w3.org/2001/XMLSchema#boolean">true</cdm1:isInvariant>
        <rdfs:range rdf:resource="https://w3id.org/isoiec5087-2/v1/PersonId"/>
    </owl:ObjectProperty>
    

    <!-- Issue: ISO/IEC 5087-2 does not define the hasQualification property and it is unclear what the purpose of this is. -->
    <!-- https://w3id.org/isoiec5087-2/v1/hasQualification -->

    <owl:ObjectProperty rdf:about="https://w3id.org/isoiec5087-2/v1/hasQualification">
        <rdfs:subPropertyOf rdf:resource="https://w3id.org/isoiec5087-2/v1/CityObjectProperty"/>
    </owl:ObjectProperty>
    

    <!-- https://w3id.org/isoiec5087-2/v1/hasSkill -->

    <owl:ObjectProperty rdf:about="https://w3id.org/isoiec5087-2/v1/hasSkill">
        <skos:definition>Identifies the skills of a person.</skos:definition>
        <rdfs:subPropertyOf rdf:resource="https://w3id.org/isoiec5087-2/v1/PersonObjectProperty"/>
        <rdfs:range rdf:resource="https://w3id.org/isoiec5087-2/v1/Skill"/>
    </owl:ObjectProperty>
    

    <!-- https://w3id.org/isoiec5087-2/v1/income -->

    <owl:ObjectProperty rdf:about="https://w3id.org/isoiec5087-2/v1/income">
        <skos:definition>Identifies the annual income of a person.</skos:definition>
        <rdfs:subPropertyOf rdf:resource="https://w3id.org/isoiec5087-2/v1/PersonObjectProperty"/>
        <rdfs:range rdf:resource="https://w3id.org/isoiec5087-1/v1/MonetaryValue"/>
    </owl:ObjectProperty>
    

    <!-- https://w3id.org/isoiec5087-2/v1/issuedBy -->

    <owl:ObjectProperty rdf:about="https://w3id.org/isoiec5087-2/v1/issuedBy">
        <skos:definition>Identifies the Agent that issued the ID, document, or certificate.</skos:definition>
        <rdfs:subPropertyOf rdf:resource="https://w3id.org/isoiec5087-2/v1/PersonObjectProperty"/>
        <rdfs:range rdf:resource="https://w3id.org/isoiec5087-1/v1/Agent"/>
    </owl:ObjectProperty>
    

    <!-- Issue: Broadened the definition so that the name can be associated with any object -->
    <!-- https://w3id.org/isoiec5087-2/v1/name -->

    <owl:ObjectProperty rdf:about="https://w3id.org/isoiec5087-2/v1/name">
        <skos:definition>Identifies the instance of Name legally associated with an Object (e.g., Person).</skos:definition>
        <rdfs:subPropertyOf rdf:resource="https://w3id.org/isoiec5087-2/v1/CityObjectProperty"/>
        <rdfs:range rdf:resource="https://w3id.org/isoiec5087-2/v1/Name"/>
    </owl:ObjectProperty>
    

    <!-- https://w3id.org/isoiec5087-2/v1/parent -->

    <owl:ObjectProperty rdf:about="https://w3id.org/isoiec5087-2/v1/parent">
        <skos:definition>Identifies any parents of the Person.</skos:definition>
        <skos:note>The parent relation is broadly defined and can include legal and biological relations.</skos:note>
        <rdfs:subPropertyOf rdf:resource="https://w3id.org/isoiec5087-2/v1/PersonObjectProperty"/>
        <rdfs:range rdf:resource="https://w3id.org/isoiec5087-2/v1/Person"/>
    </owl:ObjectProperty>
    

    <!-- Issue: I changed gender to be a skos:closeMatch rather than owl:equivalentProperty; neither is defined in ISO/IEC 5087-2 (or the invariant aspect) -->
    <!-- https://w3id.org/isoiec5087-2/v1/sex -->

    <owl:ObjectProperty rdf:about="https://w3id.org/isoiec5087-2/v1/sex">
        <skos:definition>Identifies a Person's sex assigned at birth.</skos:definition>
        <skos:note>Sex is typically assigned based on a person's reproductive system and other physical characteristics. The definition of sex is distinct from that of a person’s gender; a Person is associated with at most one sex.</skos:note>
        <skos:closeMatch rdf:resource="http://schema.org/gender"/>
        <rdfs:subPropertyOf rdf:resource="https://w3id.org/isoiec5087-2/v1/PersonObjectProperty"/>
        <cdm1:isInvariant rdf:datatype="http://www.w3.org/2001/XMLSchema#boolean">true</cdm1:isInvariant>
    </owl:ObjectProperty>
    

    <!-- https://w3id.org/isoiec5087-2/v1/spouse -->

    <owl:ObjectProperty rdf:about="https://w3id.org/isoiec5087-2/v1/spouse">
        <skos:definition>Identifies any spouses of the person.</skos:definition>
        <rdfs:subPropertyOf rdf:resource="https://w3id.org/isoiec5087-2/v1/PersonObjectProperty"/>
        <rdfs:range rdf:resource="https://w3id.org/isoiec5087-2/v1/Person"/>
    </owl:ObjectProperty>
    

    <!-- https://w3id.org/isoiec5087-2/v1/validityPeriod -->

    <owl:ObjectProperty rdf:about="https://w3id.org/isoiec5087-2/v1/validityPeriod">
        <skos:definition>Identifies the time interval during which the identifier is valid.</skos:definition>
        <rdfs:subPropertyOf rdf:resource="https://w3id.org/isoiec5087-2/v1/PersonObjectProperty"/>
        <rdfs:subPropertyOf rdf:resource="http://www.w3.org/2006/time#hasTime"/>
    </owl:ObjectProperty>
    

    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Data properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    
    <!-- https://w3id.org/isoiec5087-2/v1/PersonDataProperty -->

    <owl:DatatypeProperty rdf:about="https://w3id.org/isoiec5087-2/v1/PersonDataProperty">
        <skos:definition>Added for organizational purposes, to identify data properties defined in the Person Pattern.</skos:definition>
        <rdfs:subPropertyOf rdf:resource="https://w3id.org/isoiec5087-2/v1/CityDataProperty"/>
    </owl:DatatypeProperty>
    

    <!-- https://w3id.org/isoiec5087-2/v1/additionalName -->

    <owl:DatatypeProperty rdf:about="https://w3id.org/isoiec5087-2/v1/additionalName">
        <skos:definition>Identifies a string that is the middle or additional names of the Person.</skos:definition>
        <rdfs:subPropertyOf rdf:resource="https://w3id.org/isoiec5087-2/v1/PersonDataProperty"/>
        <rdfs:domain rdf:resource="https://w3id.org/isoiec5087-2/v1/PersonName"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
    </owl:DatatypeProperty>
    

    <!-- https://w3id.org/isoiec5087-2/v1/familyName -->

    <owl:DatatypeProperty rdf:about="https://w3id.org/isoiec5087-2/v1/familyName">
        <skos:definition>Identifies as single string that is the family or last name of the Person.</skos:definition>
        <rdfs:subPropertyOf rdf:resource="https://w3id.org/isoiec5087-2/v1/PersonDataProperty"/>
        <rdfs:domain rdf:resource="https://w3id.org/isoiec5087-2/v1/PersonName"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
    </owl:DatatypeProperty>
    

    <!-- https://w3id.org/isoiec5087-2/v1/givenName -->

    <owl:DatatypeProperty rdf:about="https://w3id.org/isoiec5087-2/v1/givenName">
        <skos:definition>Identifies a string that is the given or first name of the Person.</skos:definition>
        <rdfs:subPropertyOf rdf:resource="https://w3id.org/isoiec5087-2/v1/PersonDataProperty"/>
        <rdfs:domain rdf:resource="https://w3id.org/isoiec5087-2/v1/PersonName"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
    </owl:DatatypeProperty>
    

    <!-- https://w3id.org/isoiec5087-2/v1/photoID -->

    <owl:DatatypeProperty rdf:about="https://w3id.org/isoiec5087-2/v1/photoID">
        <skos:definition>Specifies whether the identifier contains a photo.</skos:definition>
        <rdfs:subPropertyOf rdf:resource="https://w3id.org/isoiec5087-2/v1/PersonDataProperty"/>
        <rdfs:domain rdf:resource="https://w3id.org/isoiec5087-2/v1/Person"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#boolean"/>
    </owl:DatatypeProperty>
    

    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Classes
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    
    <!-- https://w3id.org/isoiec5087-2/v1/PersonThing -->

    <owl:Class rdf:about="https://w3id.org/isoiec5087-2/v1/PersonThing">
        <rdfs:subClassOf rdf:resource="https://w3id.org/isoiec5087-2/v1/CityThing"/>
        <skos:definition>Added for organizational purposes, to identify classes defined in the Person ontology.</skos:definition>
    </owl:Class>
    

    <!-- https://w3id.org/isoiec5087-2/v1/Citizenship -->

    <owl:Class rdf:about="https://w3id.org/isoiec5087-2/v1/Citizenship">
        <skos:definition>Citizenship is a type of City Thing that indicates the country of citizenship for a person during a specific period.</skos:definition>
        <rdfs:subClassOf rdf:resource="https://w3id.org/isoiec5087-2/v1/PersonThing"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="https://w3id.org/isoiec5087-2/v1/forCountry"/>
                <owl:maxQualifiedCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">1</owl:maxQualifiedCardinality>
                <owl:onClass rdf:resource="https://w3id.org/isoiec5087-2/v1/Country"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="https://w3id.org/isoiec5087-2/v1/validityPeriod"/>
                <owl:maxQualifiedCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">1</owl:maxQualifiedCardinality>
                <owl:onClass rdf:resource="http://www.w3.org/2006/time#ProperInterval"/>
            </owl:Restriction>
        </rdfs:subClassOf>
    </owl:Class>
    

    <!-- https://w3id.org/isoiec5087-2/v1/Education -->

    <owl:Class rdf:about="https://w3id.org/isoiec5087-2/v1/Education">
        <skos:definition>Education is a type of Code that describes the educational background of a person.</skos:definition>
        <rdfs:subClassOf rdf:resource="https://w3id.org/isoiec5087-2/v1/PersonThing"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="https://w3id.org/isoiec5087-2/v1/hasCode"/>
                <owl:allValuesFrom rdf:resource="https://w3id.org/isoiec5087-2/v1/Code"/>
            </owl:Restriction>
        </rdfs:subClassOf>
    </owl:Class>
    

    <!-- https://w3id.org/isoiec5087-2/v1/Gender -->

    <owl:Class rdf:about="https://w3id.org/isoiec5087-2/v1/Gender">
        <skos:definition>Gender is a type of Code that describes the gender identity of a person.</skos:definition>
        <rdfs:subClassOf rdf:resource="https://w3id.org/isoiec5087-2/v1/PersonThing"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="https://w3id.org/isoiec5087-2/v1/hasCode"/>
                <owl:allValuesFrom rdf:resource="https://w3id.org/isoiec5087-2/v1/Code"/>
            </owl:Restriction>
        </rdfs:subClassOf>
    </owl:Class>


    <!-- https://w3id.org/isoiec5087-2/v1/IDType -->

    <owl:Class rdf:about="https://w3id.org/isoiec5087-2/v1/IDType">
        <skos:definition>ID Type is a type of Code that describes the type of identification document issued to a person.</skos:definition>
        <rdfs:subClassOf rdf:resource="https://w3id.org/isoiec5087-2/v1/PersonThing"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="https://w3id.org/isoiec5087-2/v1/hasCode"/>
                <owl:allValuesFrom rdf:resource="https://w3id.org/isoiec5087-2/v1/Code"/>
            </owl:Restriction>
        </rdfs:subClassOf>
    </owl:Class>
    

    <!-- https://w3id.org/isoiec5087-2/v1/Person -->

    <owl:Class rdf:about="https://w3id.org/isoiec5087-2/v1/Person">
        <skos:definition>A Person is an individual human being.</skos:definition>
        <skos:note>Some properties are not defined in the Person ontology, but in an extension of the Person ontology that captures the interaction between iCity domain ontologies (e.g. Persons and their Vehicles)</skos:note>
        <rdfs:subClassOf rdf:resource="https://w3id.org/isoiec5087-2/v1/PersonThing"/>
        <rdfs:subClassOf rdf:resource="https://w3id.org/isoiec5087-1/v1/Agent"/>
        <rdfs:subClassOf>
            <owl:Restriction>
