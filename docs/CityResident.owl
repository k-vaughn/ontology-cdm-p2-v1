        <owl:imports rdf:resource="https://w3id.org/isoiec5087-2/v1/BuildingPattern"/>
        <owl:imports rdf:resource="https://w3id.org/isoiec5087-2/v1/OrganizationPattern"/>
        <owl:imports rdf:resource="https://w3id.org/isoiec5087-2/v1/PersonPattern"/>
    </owl:Ontology>
    

    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Object Properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    
    <!-- https://w3id.org/isoiec5087-2/v1/CityResidentObjectProperty -->

    <owl:ObjectProperty rdf:about="https://w3id.org/isoiec5087-2/v1/CityResidentObjectProperty">
        <skos:definition>Added for organizational purposes, to identify object properties defined for the City Resident Pattern in the City-level CDM ontology.</skos:definition>
        <rdfs:subPropertyOf rdf:resource="https://w3id.org/isoiec5087-2/v1/CityObjectProperty"/>
    </owl:ObjectProperty>
    

    <!-- https://w3id.org/isoiec5087-2/v1/entity -->

    <owl:ObjectProperty rdf:about="https://w3id.org/isoiec5087-2/v1/entity">
        <skos:definition>Identifies an entity that is the subject domain object.</skos:definition>
        <skos:example>In the context of a ControlledEntity, the entity property would identify the entity being controlled.</skos:example>
        <rdfs:subPropertyOf rdf:resource="https://w3id.org/isoiec5087-2/v1/CityResidentObjectProperty"/>
    </owl:ObjectProperty>
    

    <!-- https://w3id.org/isoiec5087-2/v1/forCity -->

    <owl:ObjectProperty rdf:about="https://w3id.org/isoiec5087-2/v1/forCity">
        <skos:definition>Identifies the City of the residence.</skos:definition>
        <rdfs:subPropertyOf rdf:resource="https://w3id.org/isoiec5087-2/v1/CityResidentObjectProperty"/>
        <rdfs:domain rdf:resource="https://w3id.org/isoiec5087-2/v1/Residence"/>
        <rdfs:range rdf:resource="https://w3id.org/isoiec5087-2/v1/City"/>
    </owl:ObjectProperty>
    

    <!-- https://w3id.org/isoiec5087-2/v1/hasHomeType -->

    <owl:ObjectProperty rdf:about="https://w3id.org/isoiec5087-2/v1/hasHomeType">
        <skos:definition>Identifies the type of home.</skos:definition>
        <skos:example>house, apartment, or shelter</skos:example>
        <rdfs:subPropertyOf rdf:resource="https://w3id.org/isoiec5087-2/v1/CityResidentObjectProperty"/>
        <rdfs:domain rdf:resource="https://w3id.org/isoiec5087-2/v1/Residence"/>
        <rdfs:range rdf:resource="https://w3id.org/isoiec5087-2/v1/HomeType"/>
    </owl:ObjectProperty>
    

    <!-- https://w3id.org/isoiec5087-2/v1/hasResidence -->

    <owl:ObjectProperty rdf:about="https://w3id.org/isoiec5087-2/v1/hasResidence">
        <skos:definition>Specifies a Residence of a CityResident.</skos:definition>
        <rdfs:subPropertyOf rdf:resource="https://w3id.org/isoiec5087-2/v1/CityResidentObjectProperty"/>
        <rdfs:domain rdf:resource="https://w3id.org/isoiec5087-2/v1/CityResident"/>
        <rdfs:range rdf:resource="https://w3id.org/isoiec5087-2/v1/Residence"/>
    </owl:ObjectProperty>
    

    <!-- https://w3id.org/isoiec5087-2/v1/hasResidentialRelationship -->

    <owl:ObjectProperty rdf:about="https://w3id.org/isoiec5087-2/v1/hasResidentialRelationship">
        <skos:definition>Specifies the type of residential relationship that exists.</skos:definition>
        <skos:example>ownership, rental</skos:example>
        <rdfs:subPropertyOf rdf:resource="https://w3id.org/isoiec5087-2/v1/CityResidentObjectProperty"/>
        <rdfs:domain rdf:resource="https://w3id.org/isoiec5087-2/v1/Residence"/>
        <rdfs:range rdf:resource="https://w3id.org/isoiec5087-2/v1/ResidentialRelationship"/>
    </owl:ObjectProperty>
    

    <!-- https://w3id.org/isoiec5087-2/v1/operates -->

    <owl:ObjectProperty rdf:about="https://w3id.org/isoiec5087-2/v1/operates">
        <skos:definition>Specifies an EntityOperation where the entity is an Organization that the resident operates.</skos:definition>
        <rdfs:subPropertyOf rdf:resource="https://w3id.org/isoiec5087-2/v1/CityResidentObjectProperty"/>
        <rdfs:domain rdf:resource="https://w3id.org/isoiec5087-2/v1/CityResident"/>
        <rdfs:range rdf:resource="https://w3id.org/isoiec5087-2/v1/EntityOperation"/>
    </owl:ObjectProperty>
    

    <!-- https://w3id.org/isoiec5087-2/v1/owns -->

    <owl:ObjectProperty rdf:about="https://w3id.org/isoiec5087-2/v1/owns">
        <skos:definition>Identifies an entity that is owned by another entity.</skos:definition>
        <rdfs:subPropertyOf rdf:resource="https://w3id.org/isoiec5087-2/v1/CityResidentObjectProperty"/>
        <rdfs:range rdf:resource="https://w3id.org/isoiec5087-2/v1/EntityOwnership"/>
    </owl:ObjectProperty>
    

    <!-- https://w3id.org/isoiec5087-2/v1/residentOf -->

    <owl:ObjectProperty rdf:about="https://w3id.org/isoiec5087-2/v1/residentOf">
        <skos:definition>Identifies the JurisdictionalArea that a CityResident is a resident.</skos:definition>
        <rdfs:subPropertyOf rdf:resource="https://w3id.org/isoiec5087-2/v1/CityResidentObjectProperty"/>
        <rdfs:domain rdf:resource="https://w3id.org/isoiec5087-2/v1/CityResident"/>
        <rdfs:range rdf:resource="https://w3id.org/isoiec5087-2/v1/JurisdictionalArea"/>
    </owl:ObjectProperty>
    

