    <!-- https://w3id.org/isoiec5087-2/v1/CityResidentDataProperty -->

    <owl:DatatypeProperty rdf:about="https://w3id.org/isoiec5087-2/v1/CityResidentDataProperty">
        <skos:definition>Added for organizational purposes, to identify data properties defined in the City Resident Pattern.</skos:definition>
        <rdfs:subPropertyOf rdf:resource="https://w3id.org/isoiec5087-2/v1/CityDataProperty"/>
    </owl:DatatypeProperty>
    

    <!-- https://w3id.org/isoiec5087-2/v1/percentOwnership -->

    <owl:DatatypeProperty rdf:about="https://w3id.org/isoiec5087-2/v1/percentOwnership">
        <skos:definition>Specifies the percentage of ownership that an entity has in another entity.</skos:definition>
        <rdfs:subPropertyOf rdf:resource="https://w3id.org/isoiec5087-2/v1/CityResidentDataProperty"/>
        <rdfs:domain rdf:resource="https://w3id.org/isoiec5087-2/v1/EntityOwnership"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#decimal"/>
    </owl:DatatypeProperty>
    

