    <!-- https://w3id.org/isoiec5087-2/v1/CityResidentThing -->

    <owl:Class rdf:about="https://w3id.org/isoiec5087-2/v1/CityResidentThing">
        <rdfs:subClassOf rdf:resource="https://w3id.org/isoiec5087-2/v1/CityThing"/>
        <skos:definition>Added for organizational purposes, to identify classes defined in the City Resident ontology.</skos:definition>
    </owl:Class>
    

    <!-- https://w3id.org/isoiec5087-2/v1/CityResident -->

    <owl:Class rdf:about="https://w3id.org/isoiec5087-2/v1/CityResident">
        <skos:definition>A City Resident is a Person who satisfies the requirements of being a city resident for the particular City.</skos:definition>
        <skos:note>Different cities have different definitions of who is that city's Resident, the City Resident Pattern contains the core properties required by each. For example, the city of Toronto's definition of a city resident includes the concept of owning property or owning or operating a business in the city. For Beijing, nationality is a unique aspect. Central to all the definitions is the concept of residing. </skos:note>
        <rdfs:subClassOf rdf:resource="https://w3id.org/isoiec5087-2/v1/CityResidentThing"/>
        <rdfs:subClassOf rdf:resource="https://w3id.org/isoiec5087-2/v1/Person"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="https://w3id.org/isoiec5087-2/v1/hasResidence"/>
                <owl:allValuesFrom rdf:resource="https://w3id.org/isoiec5087-2/v1/Residence"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="https://w3id.org/isoiec5087-2/v1/operates"/>
                <owl:allValuesFrom rdf:resource="https://w3id.org/isoiec5087-2/v1/EntityOperation"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="https://w3id.org/isoiec5087-2/v1/owns"/>
                <owl:allValuesFrom rdf:resource="https://w3id.org/isoiec5087-2/v1/EntityOwnership"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="https://w3id.org/isoiec5087-2/v1/residentOf"/>
                <owl:allValuesFrom rdf:resource="https://w3id.org/isoiec5087-2/v1/JurisdictionalArea"/>
            </owl:Restriction>
        </rdfs:subClassOf>
    </owl:Class>
    

    <!-- https://w3id.org/isoiec5087-2/v1/ControlledEntity -->

    <owl:Class rdf:about="https://w3id.org/isoiec5087-2/v1/ControlledEntity">
        <skos:definition>A controlled entity is an entity that is subject to control by another entity.</skos:definition>
        <rdfs:subClassOf rdf:resource="https://w3id.org/isoiec5087-2/v1/CityResidentThing"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="https://w3id.org/isoiec5087-2/v1/entity"/>
                <owl:someValuesFrom rdf:resource="http://www.w3.org/2002/07/owl#Thing"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.w3.org/2006/time#hasTime"/>
                <owl:maxQualifiedCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">1</owl:maxQualifiedCardinality>
                <owl:onClass rdf:resource="http://www.w3.org/2006/time#ProperInterval"/>
            </owl:Restriction>
        </rdfs:subClassOf>
    </owl:Class>
    

    <!-- https://w3id.org/isoiec5087-2/v1/EntityOperation -->

    <owl:Class rdf:about="https://w3id.org/isoiec5087-2/v1/EntityOperation">
        <skos:definition>Activity of operating an Organization by City Resident.</skos:definition>
        <rdfs:subClassOf rdf:resource="https://w3id.org/isoiec5087-2/v1/ControlledEntity"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="https://w3id.org/isoiec5087-2/v1/entity"/>
                <owl:maxQualifiedCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">1</owl:maxQualifiedCardinality>
                <owl:onClass rdf:resource="https://w3id.org/isoiec5087-2/v1/Organization"/>
            </owl:Restriction>
        </rdfs:subClassOf>
    </owl:Class>
    

    <!-- https://w3id.org/isoiec5087-2/v1/EntityOwnership -->

    <owl:Class rdf:about="https://w3id.org/isoiec5087-2/v1/EntityOwnership">
        <skos:definition>Activity of owning a Building, Land Area, or Organization by City Resident.</skos:definition>
        <rdfs:subClassOf rdf:resource="https://w3id.org/isoiec5087-2/v1/ControlledEntity"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="https://w3id.org/isoiec5087-2/v1/entity"/>
                <owl:maxQualifiedCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">1</owl:maxQualifiedCardinality>
                <owl:onClass>
                    <owl:Class>
                        <owl:unionOf rdf:parseType="Collection">
                            <rdf:Description rdf:about="https://w3id.org/isoiec5087-2/v1/Building"/>
                            <rdf:Description rdf:about="https://w3id.org/isoiec5087-2/v1/LandArea"/>
                            <rdf:Description rdf:about="https://w3id.org/isoiec5087-2/v1/Organization"/>
                        </owl:unionOf>
                    </owl:Class>
                </owl:onClass>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="https://w3id.org/isoiec5087-2/v1/percentOwnership"/>
                <owl:maxQualifiedCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">1</owl:maxQualifiedCardinality>
                <owl:onDataRange rdf:resource="http://www.w3.org/2001/XMLSchema#decimal"/>
            </owl:Restriction>
        </rdfs:subClassOf>
    </owl:Class>
    

    <!-- https://w3id.org/isoiec5087-2/v1/HomeType -->

    <owl:Class rdf:about="https://w3id.org/isoiec5087-2/v1/HomeType">
        <skos:definition>Home Type is a type of Code that describes the type of a Residence.</skos:definition>
        <rdfs:subClassOf rdf:resource="https://w3id.org/isoiec5087-2/v1/CityResidentThing"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="https://w3id.org/isoiec5087-2/v1/hasCode"/>
                <owl:allValuesFrom rdf:resource="https://w3id.org/isoiec5087-2/v1/Code"/>
            </owl:Restriction>
        </rdfs:subClassOf>
    </owl:Class>
    

    <!-- https://w3id.org/isoiec5087-2/v1/Residence -->

    <owl:Class rdf:about="https://w3id.org/isoiec5087-2/v1/Residence">
        <rdfs:subClassOf rdf:resource="https://w3id.org/isoiec5087-2/v1/CityResidentThing"/>
        <!-- Issue: ISO/IEC 5087-2 defines hasResidentialRelationship as max 1 rather exactly 1; keep for max 1 for now as it can be broadened to only if needed. -->
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="https://w3id.org/isoiec5087-2/v1/hasResidentialRelationship"/>
                <owl:qualifiedCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">1</owl:qualifiedCardinality>
                <owl:onClass rdf:resource="https://w3id.org/isoiec5087-2/v1/ResidentialRelationship"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="https://w3id.org/isoiec5087-2/v1/forCity"/>
                <owl:maxQualifiedCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">1</owl:maxQualifiedCardinality>
                <owl:onClass rdf:resource="https://w3id.org/isoiec5087-2/v1/City"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="https://w3id.org/isoiec5087-2/v1/hasHomeType"/>
                <owl:maxQualifiedCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">1</owl:maxQualifiedCardinality>
                <owl:onClass rdf:resource="https://w3id.org/isoiec5087-2/v1/HomeType"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="https://w3id.org/isoiec5087-2/v1/hasAddress"/>
                <owl:maxQualifiedCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">1</owl:maxQualifiedCardinality>
                <owl:onClass rdf:resource="https://w3id.org/isoiec5087-2/v1/Address"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.w3.org/2006/time#hasTime"/>
                <owl:maxQualifiedCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">1</owl:maxQualifiedCardinality>
                <owl:onClass rdf:resource="http://www.w3.org/2006/time#ProperInterval"/>
            </owl:Restriction>
        </rdfs:subClassOf>
    </owl:Class>
    

<!-- Issue: ISO/IEC 5087-2 does not define hasResidentialRelationship but references it. Keep. -->
    <!-- https://w3id.org/isoiec5087-2/v1/ResidentialRelationship -->

    <owl:Class rdf:about="https://w3id.org/isoiec5087-2/v1/ResidentialRelationship">
        <skos:definition>A ResidentialRelationship defines the relationship of a Resident to a Residence.</skos:definition>
        <rdfs:subClassOf rdf:resource="https://w3id.org/isoiec5087-2/v1/CityResidentThing"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="https://w3id.org/isoiec5087-2/v1/hasCode"/>
                <owl:allValuesFrom rdf:resource="https://w3id.org/isoiec5087-2/v1/Code"/>
            </owl:Restriction>
        </rdfs:subClassOf>
    </owl:Class>
    

