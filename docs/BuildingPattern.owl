<?xml version="1.0"?>
<rdf:RDF xmlns="https://w3id.org/citydata/part2/v1/"
     xml:base="https://w3id.org/citydata/part2/v1/"
     xmlns:cc="http://creativecommons.org/ns#"
     xmlns:dcterms="http://purl.org/dc/terms/"
     xmlns:owl="http://www.w3.org/2002/07/owl#"
     xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
     xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
     xmlns:skos="http://www.w3.org/2004/02/skos/core#"
     xmlns:vann="http://purl.org/vocab/vann/"
     xmlns:xml="http://www.w3.org/XML/1998/namespace"
     xmlns:xsd="http://www.w3.org/2001/XMLSchema#"
     xmlns:i72="https://w3id.org/citydata/21972/v1/"
     xmlns:cdm1="https://w3id.org/citydata/part1/v1/">
    <owl:Ontology rdf:about="https://w3id.org/citydata/part2/v1/BuildingPattern">
        <!-- Identity -->
        <dcterms:title>Information technology - City data model - Part 2: City level concepts - Building Pattern</dcterms:title>
        <rdfs:seeAlso rdf:resource="https://www.iso.org/standard/80742.html"/>
        <dcterms:bibliographicCitation>ISO/IEC 5087-2:2024</dcterms:bibliographicCitation>
    	<skos:definition>This ontology specifies the city-level concepts for the building pattern of the city data model. A Building is a structure with some location in the urban system. The location of the Building in space may change due to construction, but the Parcel/Lot of land it is located on cannot. There may be many different types (subclasses) of buildings, such as House, Apartment Building, Office Building, and so on.</skos:definition>
    	<vann:preferredNamespaceUri>https://w3id.org/citydata/part2/v1/</vann:preferredNamespaceUri>
    	<vann:preferredNamespacePrefix>cdm2</vann:preferredNamespacePrefix>

    	<!-- Provenance -->
        <dcterms:creator>Megan Katsumi</dcterms:creator>
        <dcterms:creator>Mark Fox</dcterms:creator>
    	<dcterms:creator>Kenneth Vaughn</dcterms:creator>
        <!-- Prior release -->
        <rdfs:comment>The prior version of this ontology used a different ontology IRI. This update remains semantically consistent with the version adopted as ISO 5087-2:2024.</rdfs:comment>
        <owl:priorVersion rdf:resource="https://standards.iso.org/iso-iec/5087/-2/ed-1/en/ontology/Building/"/>
        <!-- This release -->
		<dcterms:modified rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2026-02-12</dcterms:modified> 
        <owl:versionInfo>1.3.0</owl:versionInfo>
        <owl:versionIRI rdf:resource="https://w3id.org/citydata/part2/v1/BuildingPattern/r3.0"/>
        
        <!-- License -->
        <cc:license>http://creativecommons.org/licenses/by/4.0/</cc:license>
        
        <!-- Imports -->
        <owl:imports rdf:resource="https://w3id.org/citydata/part2/v1/InfrastructurePattern"/>
    </owl:Ontology>
    

    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Object Properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    
    <!-- https://w3id.org/citydata/part2/v1/BuildingObjectProperty -->

    <owl:ObjectProperty rdf:about="https://w3id.org/citydata/part2/v1/BuildingObjectProperty">
        <skos:definition>Added for organizational purposes, to identify object properties defined for the Building Pattern in the City-level CDM ontology.</skos:definition>
        <rdfs:subPropertyOf rdf:resource="https://w3id.org/citydata/part2/v1/CityObjectProperty"/>
    </owl:ObjectProperty>
    

    <!-- https://w3id.org/citydata/part2/v1/buildingFacility -->

    <owl:ObjectProperty rdf:about="https://w3id.org/citydata/part2/v1/buildingFacility">
        <skos:definition>Identifies a Facility that is contained in the Building.</skos:definition>
        <skos:example>kitchen, bath, or air conditioning</skos:example>
        <rdfs:subPropertyOf rdf:resource="https://w3id.org/citydata/part2/v1/BuildingObjectProperty"/>
        <rdfs:subPropertyOf rdf:resource="https://w3id.org/citydata/part1/v1/hasComponent"/>
        <rdfs:range rdf:resource="https://w3id.org/citydata/part2/v1/Facility"/>
    </owl:ObjectProperty>
    

    <!-- https://w3id.org/citydata/part2/v1/floorAreaRatio -->

    <owl:ObjectProperty rdf:about="https://w3id.org/citydata/part2/v1/floorAreaRatio">
        <skos:definition>Identifies the building's floor area ratio. This is a ratio of the gross floor area (the value of hasBuildingFloorArea) to its “buildable area” (i.e. lot size).</skos:definition>
        <rdfs:subPropertyOf rdf:resource="https://w3id.org/citydata/part2/v1/BuildingObjectProperty"/>
        <rdfs:domain rdf:resource="https://w3id.org/citydata/part2/v1/Building"/>
        <rdfs:range rdf:resource="https://w3id.org/citydata/part2/v1/AreaRatio"/>
    </owl:ObjectProperty>
    

    <!-- https://w3id.org/citydata/part2/v1/floorToCeilingHeight -->

    <owl:ObjectProperty rdf:about="https://w3id.org/citydata/part2/v1/floorToCeilingHeight">
        <skos:definition>Identifies the floor to ceiling height in the Building Unit.</skos:definition>
        <rdfs:subPropertyOf rdf:resource="https://w3id.org/citydata/part2/v1/BuildingObjectProperty"/>
        <rdfs:domain rdf:resource="https://w3id.org/citydata/part2/v1/BuildingUnit"/>
        <rdfs:range rdf:resource="https://w3id.org/citydata/part1/v1/Length"/>
    </owl:ObjectProperty>
    

    <!-- https://w3id.org/citydata/part2/v1/hasBuildingFloorArea -->

    <owl:ObjectProperty rdf:about="https://w3id.org/citydata/part2/v1/hasBuildingFloorArea">
        <skos:definition>identifies the floor area occupied by the Building. The floor area accounts for the area of each floor of the building. However, floor area excludes unoccupied areas such as basements.</skos:definition>
        <rdfs:subPropertyOf rdf:resource="https://w3id.org/citydata/part2/v1/BuildingObjectProperty"/>
        <rdfs:domain rdf:resource="https://w3id.org/citydata/part2/v1/Building"/>
        <rdfs:range rdf:resource="https://w3id.org/citydata/part1/v1/Area"/>
    </owl:ObjectProperty>
    

    <!-- https://w3id.org/citydata/part2/v1/hasBuildingFootprintArea -->

    <owl:ObjectProperty rdf:about="https://w3id.org/citydata/part2/v1/hasBuildingFootprintArea">
        <skos:definition>Identifies the footprint occupied by the Building.</skos:definition>
        <rdfs:subPropertyOf rdf:resource="https://w3id.org/citydata/part2/v1/BuildingObjectProperty"/>
        <rdfs:domain rdf:resource="https://w3id.org/citydata/part2/v1/Building"/>
        <rdfs:range rdf:resource="https://w3id.org/citydata/part1/v1/Area"/>
    </owl:ObjectProperty>
    

    <!-- https://w3id.org/citydata/part2/v1/hasBuildingHeight -->

    <owl:ObjectProperty rdf:about="https://w3id.org/citydata/part2/v1/hasBuildingHeight">
        <skos:definition>Identifies the Building’s height.</skos:definition>
        <rdfs:subPropertyOf rdf:resource="https://w3id.org/citydata/part2/v1/BuildingObjectProperty"/>
        <rdfs:domain rdf:resource="https://w3id.org/citydata/part2/v1/Building"/>
        <rdfs:range rdf:resource="https://w3id.org/citydata/part1/v1/Length"/>
    </owl:ObjectProperty>
    

    <!-- https://w3id.org/citydata/part2/v1/hasBuildingUnit -->

    <owl:ObjectProperty rdf:about="https://w3id.org/citydata/part2/v1/hasBuildingUnit">
        <skos:definition>Identifies the Building Unit(s), if any, that are contained by the Building.</skos:definition>
        <rdfs:subPropertyOf rdf:resource="https://w3id.org/citydata/part2/v1/BuildingObjectProperty"/>
        <rdfs:subPropertyOf rdf:resource="https://w3id.org/citydata/part1/v1/hasComponent"/>
        <rdfs:domain rdf:resource="https://w3id.org/citydata/part2/v1/Building"/>
        <rdfs:range rdf:resource="https://w3id.org/citydata/part2/v1/BuildingUnit"/>
    </owl:ObjectProperty>
    

    <!-- Issue: v1.2.0 defined hasConstructionStatus to be a subProperty of hasComponent, but this was not in ISO/IEC 5087-2 and the restriction seems to be in error. It has been removed. -->
    <!-- https://w3id.org/citydata/part2/v1/hasConstructionStatus -->

    <owl:ObjectProperty rdf:about="https://w3id.org/citydata/part2/v1/hasConstructionStatus">
        <skos:definition>identifies the Construction Status of a Building.</skos:definition>
        <rdfs:subPropertyOf rdf:resource="https://w3id.org/citydata/part2/v1/BuildingObjectProperty"/>
        <rdfs:domain rdf:resource="https://w3id.org/citydata/part2/v1/Building"/>
        <rdfs:range rdf:resource="https://w3id.org/citydata/part2/v1/ConstructionStatus"/>
    </owl:ObjectProperty>
    

    <!-- hasFunction is not defined in ISO/IEC 5087-2 and is not used in this document. Recommend deletion. -->
    <!-- https://w3id.org/citydata/part2/v1/hasFunction -->

<!--     <owl:ObjectProperty rdf:about="https://w3id.org/citydata/part2/v1/hasFunction">
        <rdfs:subPropertyOf rdf:resource="https://w3id.org/citydata/part2/v1/CityObjectProperty"/>
    </owl:ObjectProperty>  -->
    

    <!-- https://w3id.org/citydata/part2/v1/hasRent -->

    <owl:ObjectProperty rdf:about="https://w3id.org/citydata/part2/v1/hasRent">
        <skos:definition>Identifies the rental fee for the Building Unit.</skos:definition>
        <rdfs:subPropertyOf rdf:resource="https://w3id.org/citydata/part2/v1/BuildingObjectProperty"/>
        <rdfs:domain rdf:resource="https://w3id.org/citydata/part2/v1/BuildingUnit"/>
        <rdfs:range rdf:resource="https://w3id.org/citydata/part1/v1/MonetaryValue"/>
    </owl:ObjectProperty>
    

    <!-- Issue: v1.2.0 called this unitSize but it was changed to align with ISO/IEC 5087-2 -->
    <!-- https://w3id.org/citydata/part2/v1/hasUnitSize -->

    <owl:ObjectProperty rdf:about="https://w3id.org/citydata/part2/v1/hasUnitSize">
        <skos:definition>Identifies the size of the Building Unit as an Area.</skos:definition>
        <rdfs:subPropertyOf rdf:resource="https://w3id.org/citydata/part2/v1/BuildingObjectProperty"/>
        <rdfs:domain rdf:resource="https://w3id.org/citydata/part2/v1/BuildingUnit"/>
        <rdfs:range rdf:resource="https://w3id.org/citydata/part1/v1/Area"/>
    </owl:ObjectProperty>
    

    <!-- locatedOn is not defined in ISO/IEC 5087-2 nor is it referenced in this file. Recommend deletion. -->
    <!-- https://w3id.org/citydata/part2/v1/locatedOn -->

<!--     <owl:ObjectProperty rdf:about="https://w3id.org/citydata/part2/v1/locatedOn">
        <rdfs:subPropertyOf rdf:resource="https://w3id.org/citydata/part2/v1/BuildingObjectProperty"/>
        <skos:definition>A Building is located on some parcel of land.</skos:definition>
    </owl:ObjectProperty>  -->
    

    <!-- https://w3id.org/citydata/part2/v1/unitInBuilding -->

    <owl:ObjectProperty rdf:about="https://w3id.org/citydata/part2/v1/unitInBuilding">
        <skos:definition>Identifies the Building that the Building Unit belongs to (is contained in).</skos:definition>
        <rdfs:subPropertyOf rdf:resource="https://w3id.org/citydata/part2/v1/BuildingObjectProperty"/>
        <owl:inverseOf rdf:resource="https://w3id.org/citydata/part2/v1/hasBuildingUnit"/>
    </owl:ObjectProperty>
    

    <!-- https://w3id.org/citydata/part2/v1/use -->

    <owl:ObjectProperty rdf:about="https://w3id.org/citydata/part2/v1/use">
        <skos:definition>Identifies the use of the Building, typically associated with some classification system.</skos:definition>
        <rdfs:subPropertyOf rdf:resource="https://w3id.org/citydata/part2/v1/BuildingObjectProperty"/>
        <rdfs:domain rdf:resource="https://w3id.org/citydata/part2/v1/Building"/>
        <rdfs:range rdf:resource="https://w3id.org/citydata/part2/v1/BuildingUse"/>
    </owl:ObjectProperty>
    

    <!-- https://w3id.org/citydata/part2/v1/windowToWallRatio -->

    <owl:ObjectProperty rdf:about="https://w3id.org/citydata/part2/v1/windowToWallRatio">
        <skos:definition>Identifies the percentage area of a building’s exterior envelope that is made up of glazing (i.e., glass installed in fixed openings such as windows and doors).</skos:definition>
        <rdfs:subPropertyOf rdf:resource="https://w3id.org/citydata/part2/v1/BuildingObjectProperty"/>
        <rdfs:domain rdf:resource="https://w3id.org/citydata/part2/v1/Building"/>
        <rdfs:range rdf:resource="https://w3id.org/citydata/21972/v1/RatioIndicator"/>
    </owl:ObjectProperty>
    

    <!-- https://w3id.org/citydata/part2/v1/yearOfConstruction -->

    <owl:ObjectProperty rdf:about="https://w3id.org/citydata/part2/v1/yearOfConstruction">
        <skos:definition>Identifies the year that construction on the Building was completed.</skos:definition>
        <rdfs:subPropertyOf rdf:resource="https://w3id.org/citydata/part2/v1/BuildingObjectProperty"/>
        <rdfs:domain rdf:resource="https://w3id.org/citydata/part2/v1/Building"/>
        <rdfs:range rdf:resource="https://w3id.org/citydata/part2/v1/Year"/>
    </owl:ObjectProperty>
    

    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Data properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    
    <!-- https://w3id.org/citydata/part2/v1/BuildingDataProperty -->

    <owl:DatatypeProperty rdf:about="https://w3id.org/citydata/part2/v1/BuildingDataProperty">
        <skos:definition>Added for organizational purposes, to identify data properties defined in the Building Pattern.</skos:definition>
        <rdfs:subPropertyOf rdf:resource="https://w3id.org/citydata/part2/v1/CityDataProperty"/>
    </owl:DatatypeProperty>
    

    <!-- https://w3id.org/citydata/part2/v1/builtAccordingToConstructionCode -->

    <owl:DatatypeProperty rdf:about="https://w3id.org/citydata/part2/v1/builtAccordingToConstructionCode">
        <skos:definition>Identifies the name of any construction code(s) applicable during the construction of the Building. </skos:definition>
        <skos:note>Construction codes refer to a set of rules or instructions that specify the standards for constructed objects such as buildings and non-building structures. Buildings must conform to the code throughout the construction of the building.</skos:note>
        <rdfs:subPropertyOf rdf:resource="https://w3id.org/citydata/part2/v1/BuildingDataProperty"/>
        <rdfs:domain rdf:resource="https://w3id.org/citydata/part2/v1/Building"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
    </owl:DatatypeProperty>
    

    <!-- Issue: Ambiguity: Does the "ground floor" count as an above ground floor? (i.e., "above-ground floors" or "above the ground floor"? -->
    <!-- https://w3id.org/citydata/part2/v1/numAboveGroundFloors -->

    <owl:DatatypeProperty rdf:about="https://w3id.org/citydata/part2/v1/numAboveGroundFloors">
        <skos:definition>Specifies the number of above-ground floors in a building, including the ground floor.</skos:definition>
        <rdfs:subPropertyOf rdf:resource="https://w3id.org/citydata/part2/v1/BuildingDataProperty"/>
        <rdfs:domain rdf:resource="https://w3id.org/citydata/part2/v1/Building"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#nonNegativeInteger"/>
    </owl:DatatypeProperty>
    

    <!-- https://w3id.org/citydata/part2/v1/numBuildingUnits -->

    <owl:DatatypeProperty rdf:about="https://w3id.org/citydata/part2/v1/numBuildingUnits">
        <skos:definition>Specifies the total number of building units in a building.</skos:definition>
        <rdfs:subPropertyOf rdf:resource="https://w3id.org/citydata/part2/v1/BuildingDataProperty"/>
        <rdfs:domain rdf:resource="https://w3id.org/citydata/part2/v1/Building"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#nonNegativeInteger"/>
    </owl:DatatypeProperty>
    

    <!-- https://w3id.org/citydata/part2/v1/numFloors -->

    <owl:DatatypeProperty rdf:about="https://w3id.org/citydata/part2/v1/numFloors">
        <skos:definition>Identifies the total number of floors in a building (including those below ground).</skos:definition>
        <rdfs:subPropertyOf rdf:resource="https://w3id.org/citydata/part2/v1/BuildingDataProperty"/>
        <rdfs:domain rdf:resource="https://w3id.org/citydata/part2/v1/Building"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#nonNegativeInteger"/>
    </owl:DatatypeProperty>
    

    <!-- https://w3id.org/citydata/part2/v1/numberOfBedrooms -->

    <owl:DatatypeProperty rdf:about="https://w3id.org/citydata/part2/v1/numberOfBedrooms">
        <skos:definition>Specifies the number of bedrooms in a building unit.</skos:definition>
        <rdfs:subPropertyOf rdf:resource="https://w3id.org/citydata/part2/v1/BuildingDataProperty"/>
        <rdfs:domain rdf:resource="https://w3id.org/citydata/part2/v1/BuildingUnit"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#nonNegativeInteger"/>
    </owl:DatatypeProperty>
    

    <!-- https://w3id.org/citydata/part2/v1/numberOfRooms -->

    <owl:DatatypeProperty rdf:about="https://w3id.org/citydata/part2/v1/numberOfRooms">
        <skos:definition>Specifies the number of rooms in a building unit.</skos:definition>
        <rdfs:subPropertyOf rdf:resource="https://w3id.org/citydata/part2/v1/BuildingDataProperty"/>
        <rdfs:domain rdf:resource="https://w3id.org/citydata/part2/v1/BuildingUnit"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#nonNegativeInteger"/>
    </owl:DatatypeProperty>
    

    <!-- https://w3id.org/citydata/part2/v1/propertyRegistrationID -->

    <owl:DatatypeProperty rdf:about="https://w3id.org/citydata/part2/v1/propertyRegistrationID">
        <skos:definition>Identifies the unique identifier of the real estate in the property register of authority where the Building is located.</skos:definition>
        <rdfs:subPropertyOf rdf:resource="https://w3id.org/citydata/part2/v1/BuildingDataProperty"/>
        <rdfs:domain rdf:resource="https://w3id.org/citydata/part2/v1/Building"/>
        <rdfs:range rdf:resource="http://www.w3.org/2000/01/rdf-schema#Literal"/>
    </owl:DatatypeProperty>
    

    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Classes
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    
    <!-- https://w3id.org/citydata/part2/v1/BuildingThing -->

    <owl:Class rdf:about="https://w3id.org/citydata/part2/v1/BuildingThing">
        <skos:definition>Added for organizational purposes, to identify classes defined in the Building ontology.</skos:definition>
        <rdfs:subClassOf rdf:resource="https://w3id.org/citydata/part2/v1/CityThing"/>
    </owl:Class>
    

    <!-- https://w3id.org/citydata/part2/v1/AreaRatio -->

    <owl:Class rdf:about="https://w3id.org/citydata/part2/v1/AreaRatio">
        <rdfs:subClassOf rdf:resource="https://w3id.org/citydata/part2/v1/BuildingThing"/>
        <owl:intersectionOf rdf:parseType="Collection">
            <rdf:Description rdf:about="https://w3id.org/citydata/21972/v1/RatioIndicator"/>
            <owl:Restriction>
                <owl:onProperty rdf:resource="https://w3id.org/citydata/21972/v1/denominator"/>
                <owl:allValuesFrom rdf:resource="https://w3id.org/citydata/part1/v1/Area"/>
            </owl:Restriction>
            <owl:Restriction>
                <owl:onProperty rdf:resource="https://w3id.org/citydata/21972/v1/numerator"/>
                <owl:allValuesFrom rdf:resource="https://w3id.org/citydata/part1/v1/Area"/>
            </owl:Restriction>
        </owl:intersectionOf>
    </owl:Class>


    <!-- https://w3id.org/citydata/part2/v1/Building -->

    <owl:Class rdf:about="https://w3id.org/citydata/part2/v1/Building">
        <skos:definition>A Building is a type of Infrastructure Element that is a structure with a roof and walls, such as a house, school, or factory. The location of a Building may change due to construction, but the Parcel/Lot of land it is located on cannot (i.e., moving an entire building results in a change in object instance). </skos:definition>
        <skos:note>There are different types (subclasses) of buildings; House, Apartment Building, Office Building...</skos:note>
        <rdfs:subClassOf rdf:resource="https://w3id.org/citydata/part2/v1/InfrastructureElement"/>
        <rdfs:subClassOf rdf:resource="https://w3id.org/citydata/part2/v1/BuildingThing"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="https://w3id.org/citydata/part2/v1/yearOfConstruction"/>
                <owl:maxQualifiedCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">1</owl:maxQualifiedCardinality>
                <owl:onClass rdf:resource="https://w3id.org/citydata/part2/v1/Year"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="https://w3id.org/citydata/part2/v1/buildingFacility"/>
                <owl:allValuesFrom rdf:resource="https://w3id.org/citydata/part2/v1/Facility"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="https://w3id.org/citydata/part2/v1/hasBuildingFloorArea"/>
                <owl:allValuesFrom rdf:resource="https://w3id.org/citydata/part1/v1/Area"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="https://w3id.org/citydata/part2/v1/hasBuildingFootprintArea"/>
                <owl:allValuesFrom rdf:resource="https://w3id.org/citydata/part1/v1/Area"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="https://w3id.org/citydata/part2/v1/hasBuildingHeight"/>
                <owl:allValuesFrom rdf:resource="https://w3id.org/citydata/part1/v1/Length"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="https://w3id.org/citydata/part2/v1/hasBuildingUnit"/>
                <owl:allValuesFrom rdf:resource="https://w3id.org/citydata/part2/v1/BuildingUnit"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="https://w3id.org/citydata/part2/v1/use"/>
                <owl:allValuesFrom rdf:resource="https://w3id.org/citydata/part2/v1/BuildingUse"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="https://w3id.org/citydata/part2/v1/floorAreaRatio"/>
                <owl:maxQualifiedCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">1</owl:maxQualifiedCardinality>
                <owl:onClass rdf:resource="https://w3id.org/citydata/part2/v1/AreaRatio"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="https://w3id.org/citydata/part2/v1/hasConstructionStatus"/>
                <owl:maxQualifiedCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">1</owl:maxQualifiedCardinality>
                <owl:onClass rdf:resource="https://w3id.org/citydata/part2/v1/ConstructionStatus"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="https://w3id.org/citydata/part2/v1/windowToWallRatio"/>
                <owl:maxQualifiedCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">1</owl:maxQualifiedCardinality>
                <owl:onClass rdf:resource="https://w3id.org/citydata/part2/v1/AreaRatio"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="https://w3id.org/citydata/part2/v1/builtAccordingToConstructionCode"/>
                <owl:allValuesFrom rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="https://w3id.org/citydata/part2/v1/numFloors"/>
                <!-- Issue: ISO/IEC 5087-2 indicates max 1 rather than exactly 1 -->
                <owl:qualifiedCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">1</owl:qualifiedCardinality>
                <owl:onDataRange rdf:resource="http://www.w3.org/2001/XMLSchema#nonNegativeInteger"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="https://w3id.org/citydata/part2/v1/numAboveGroundFloors"/>
                <owl:maxQualifiedCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">1</owl:maxQualifiedCardinality>
                <owl:onDataRange rdf:resource="http://www.w3.org/2001/XMLSchema#nonNegativeInteger"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="https://w3id.org/citydata/part2/v1/numBuildingUnits"/>
                <owl:maxQualifiedCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">1</owl:maxQualifiedCardinality>
                <owl:onDataRange rdf:resource="http://www.w3.org/2001/XMLSchema#nonNegativeInteger"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="https://w3id.org/citydata/part2/v1/propertyRegistrationID"/>
                <owl:maxCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">1</owl:maxCardinality>
            </owl:Restriction>
        </rdfs:subClassOf>
    </owl:Class>
    

    <!-- ISO/IEC 5087-2 does not define BuildingFunction, this is a null definition, and it is not used by this document. Recommend deletion. -->
    <!-- https://w3id.org/citydata/part2/v1/BuildingFunction -->

<!--     <owl:Class rdf:about="https://w3id.org/citydata/part2/v1/BuildingFunction">
        <rdfs:subClassOf rdf:resource="https://w3id.org/citydata/part2/v1/BuildingThing"/>
    </owl:Class>  -->
    

    <!-- Issue: In ISO/IEC 5087-2, BuildingUnit is not a Manifestation and it is the only time that Manifestation is used in either part 1 or part 2. Recommend deleting this restriction. -->
    <!-- https://w3id.org/citydata/part2/v1/BuildingUnit -->

    <owl:Class rdf:about="https://w3id.org/citydata/part2/v1/BuildingUnit">
        <skos:definition>A part of a Building which may be occupied by some Persons or Organization.</skos:definition>
        <rdfs:subClassOf rdf:resource="https://w3id.org/citydata/part2/v1/BuildingThing"/>
        <rdfs:subClassOf rdf:resource="https://w3id.org/citydata/part1/v1/Manifestation"/>
        <rdfs:subClassOf rdf:resource="https://w3id.org/citydata/part2/v1/InfrastructureElement"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="https://w3id.org/citydata/part2/v1/buildingFacility"/>
                <owl:allValuesFrom rdf:resource="https://w3id.org/citydata/part2/v1/Facility"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="https://w3id.org/citydata/part2/v1/floorToCeilingHeight"/>
                <owl:allValuesFrom rdf:resource="https://w3id.org/citydata/part1/v1/Length"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="https://w3id.org/citydata/part2/v1/hasRent"/>
                <owl:allValuesFrom rdf:resource="https://w3id.org/citydata/part1/v1/MonetaryValue"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="https://w3id.org/citydata/part2/v1/hasUnitSize"/>
                <owl:allValuesFrom rdf:resource="https://w3id.org/citydata/part1/v1/Area"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <!-- Issue: In ISO/IEC 5087-2, hasAddress is max 1 Address rather than only Address, recommend changing this to max 1 -->
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="https://w3id.org/citydata/part2/v1/hasAddress"/>
                <owl:allValuesFrom rdf:resource="https://w3id.org/citydata/part2/v1/Address"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <!-- Issue: In ISO/IEC 5087-2 and in this file, "hasValue only MonetaryValue" is property of InfrastructureElement; it is redundant to declare it here. Recommend deleting this declaration. -->
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="https://w3id.org/citydata/21972/v1/hasValue"/>
                <owl:allValuesFrom rdf:resource="https://w3id.org/citydata/part1/v1/MonetaryValue"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <!-- Issue: In ISO/IEC 5087-2, unitInBuilding is max 1 Building rather than exactly 1 Building as shown here. Recommand making it max 1 Building. -->
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="https://w3id.org/citydata/part2/v1/unitInBuilding"/>
                <owl:qualifiedCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">1</owl:qualifiedCardinality>
                <owl:onClass rdf:resource="https://w3id.org/citydata/part2/v1/Building"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <!-- Issue: In ISO/IEC 5087-2, existsAt is not defined as a property of BuildingUnit. It is a property of Manifestation and that is probably a better way to model the intent. Recommend deleting it from this class. -->
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="https://w3id.org/citydata/part1/v1/existsAt"/>
                <owl:qualifiedCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">1</owl:qualifiedCardinality>
                <owl:onClass rdf:resource="http://www.w3.org/2006/time#TemporalEntity"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <!-- Issue: In ISO/IEC 5087-2, numberOfBedrooms is max 1 nonNegativeInteger rather than only nonNegativeInteger as shown here. Recommand making it max 1 nonNegativeInteger. -->
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="https://w3id.org/citydata/part2/v1/numberOfBedrooms"/>
                <owl:allValuesFrom rdf:resource="http://www.w3.org/2001/XMLSchema#nonNegativeInteger"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <!-- Issue: In ISO/IEC 5087-2, numberOfRooms is max 1 nonNegativeInteger rather than only nonNegativeInteger as shown here. Recommand making it max 1 nonNegativeInteger. -->
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="https://w3id.org/citydata/part2/v1/numberOfRooms"/>
                <owl:allValuesFrom rdf:resource="http://www.w3.org/2001/XMLSchema#nonNegativeInteger"/>
            </owl:Restriction>
        </rdfs:subClassOf>
    </owl:Class>
    

    <!-- https://w3id.org/citydata/part2/v1/BuildingUse -->

    <owl:Class rdf:about="https://w3id.org/citydata/part2/v1/BuildingUse">
        <skos:definition>Building Use is a type of Code that describes the use or function of a Building</skos:definition>
        <skos:example>residential, commercial, industrial</skos:example>
        <rdfs:subClassOf rdf:resource="https://w3id.org/citydata/part2/v1/BuildingThing"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="https://w3id.org/citydata/part2/v1/hasCode"/>
                <owl:allValuesFrom rdf:resource="https://w3id.org/citydata/part2/v1/Code"/>
            </owl:Restriction>
        </rdfs:subClassOf>
    </owl:Class>
    

    <!-- https://w3id.org/citydata/part2/v1/ConstructionStatus -->

    <owl:Class rdf:about="https://w3id.org/citydata/part2/v1/ConstructionStatus">
        <skos:definition>Construction Status is a type of Code that describes the construction status of a Building or Infrastructure Element.</skos:definition>
        <rdfs:subClassOf rdf:resource="https://w3id.org/citydata/part2/v1/BuildingThing"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="https://w3id.org/citydata/part2/v1/hasCode"/>
                <owl:allValuesFrom rdf:resource="https://w3id.org/citydata/part2/v1/Code"/>
            </owl:Restriction>
        </rdfs:subClassOf>
    </owl:Class>
    

    <!-- https://w3id.org/citydata/part2/v1/Facility -->

    <owl:Class rdf:about="https://w3id.org/citydata/part2/v1/Facility">
        <skos:definition>A facility is a physical location or structure that provides services or amenities to the public or a specific group of people.</skos:definition>
        <rdfs:subClassOf rdf:resource="https://w3id.org/citydata/part2/v1/InfrastructureElement"/>
        <rdfs:subClassOf rdf:resource="https://w3id.org/citydata/part2/v1/BuildingThing"/>
        <!-- Issue: The hasLocation property is defined as max 1 Location in ISO/IEC 5087-2 rather than exactly 1 Location. Recommend changing to max 1 as it may be mobile and undesirable to show a location. -->
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="https://w3id.org/citydata/part1/v1/hasLocation"/>
                <owl:qualifiedCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">1</owl:qualifiedCardinality>
                <owl:onClass rdf:resource="https://w3id.org/citydata/part1/v1/Location"/>
            </owl:Restriction>
        </rdfs:subClassOf>
    </owl:Class>


<!--  https://w3id.org/citydata/part2/v1/Year -->
    <owl:Class rdf:about="https://w3id.org/citydata/part2/v1/Year">
        <skos:definition>Represents a year in the Gregorian calendar.</skos:definition>
        <rdfs:subClassOf rdf:resource="https://w3id.org/citydata/part2/v1/BuildingThing"/>
        <rdfs:subClassOf rdf:resource="http://www.w3.org/2006/time#DateTimeDescription"/>
</owl:Class>

